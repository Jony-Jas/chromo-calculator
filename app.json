{
  "expo": {
    "name": "Chromo Calculator",
    "slug": "chromo-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "splash": {   
        "image": "./assets/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "splash": {
        "ldpi": "./assets/drawable-ldpi/splash.png",
        "mdpi": "./assets/drawable-mdpi/splash.png",
        "hdpi": "./assets/drawable-hdpi/splash.png",
        "xhdpi": "./assets/drawable-xhdpi/splash.png",
        "xxhdpi": "./assets/drawable-xxhdpi/splash.png",
        "xxxhdpi": "./assets/drawable-xxxhdpi/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png"
      },
      "package": "com.jonyjas.chromocalculator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
